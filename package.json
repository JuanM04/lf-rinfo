{
    "name": "lf-rinfo",
    "displayName": "lf-rinfo",
    "description": "soporte para el lenguaje R-Info utilizado en la Facultad de Informatica de la UNLP. Desarrollado por La Fuente - CEFI",
    "version": "0.0.1",
    "publisher": "lafuenteunlp",
    "author": {
        "name": "La Fuente UNLP",
        "email": "lafuenteunlp@gmail.com",
        "url": "https://lafuenteunlp.com.ar"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/laFuenteUNLP/lf-rinfo.git"
    },
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "snippets",
        "craft",
        "r-info",
        "rinfo"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ri",
                "aliases": [
                    "rinfo",
                    "ri"
                ],
                "extensions": [
                    ".ri"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ri",
                "scopeName": "source.rinfo",
                "path": "./syntaxes/ri.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "ri",
                "path": "./snippets.json"
            }
        ]
    }
}
